
<!DOCTYPE html>
<head>
<%- include ("head.ejs") %>
</head>
<script src="//developers.kakao.com/sdk/js/kakao.min.js"></script>
<script type="text/javascript">
	function openPopupWindow() {
	 document.domain = "127.0.0.1"; //document.domain 값이 팝업과 부모창 동일해야 합니다.
	 window.open("/signup", "popup", "width=500, height=500, resizable=no, scrollbars=no")
	}
</script>
<body leftmargin="0" topmargin="0">
<DIV ID="objContents">
<%- include ("ddScript.ejs") %>

<div id="wrap">
	<div id="header">
	  <%- include ("header.ejs") %>
	</div><!--// header -->

	<div id="visual_wrap">
		<link rel="stylesheet" href="/css/main_slider.css" type="text/css">
		<script type="text/javascript" src="/css/js/bxslider.js"></script>
		<ul id="visual">
			<li><p style="background:url(/images/slide1_lg.jpg) top center no-repeat; height:610px;"></p></li>
			<li><p style="background:url(/images/slide2_lg.jpg) top center no-repeat; height:610px;"></p></li>
		</ul>

		<script type="text/javascript">
			$(document).ready(function(){
				$('#visual').bxSlider({
					mode:'fade',
					autoControls: true
				});
			});
		</script>
	</div><!--// visual_wrap -->
	<div class="main_container clfix">
		<%- include ("oclass.ejs") %>
		<div class="left_contents">
			<%- include ("notice.ejs") %>
			<%- include ("product.ejs") %>
		</div><!--// left_contents -->

		<div class="right_contents">
			<%- include ("gallery.ejs") %>
		</div><!--// right_contents -->
	</div><!--// main_container -->
	<div id="footer">
	  <%- include ("footer.ejs") %>
	</div><!--// footer -->
</div><!--// wrap -->


</DIV>
<DIV ID="objSelection">
</DIV>

</body>
</html>
