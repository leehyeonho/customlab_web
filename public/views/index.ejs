
<!DOCTYPE html>
<head>
<%- include ("head.ejs") %>
</head>
<script src="//developers.kakao.com/sdk/js/kakao.min.js"></script>
<script type="text/javascript">
	function openPopupWindow() {
	 document.domain = "127.0.0.1"; //document.domain 값이 팝업과 부모창 동일해야 합니다.
	 window.open("/signup", "popup", "width=500, height=500, resizable=no, scrollbars=no")
	}
</script>
<body leftmargin="0" topmargin="0">
<DIV ID="objContents">
<%- include ("ddScript.ejs") %>

<div id="wrap">
	<div id="header">
	  <%- include ("header.ejs") %>
	</div><!--// header -->

	<div id="visual_wrap">
		<link rel="stylesheet" href="/css/main_slider.css" type="text/css">
		<link rel="stylesheet" href="/css/login.css" type="text/css">
		<script type="text/javascript" src="/css/js/bxslider.js"></script>
		<ul id="visual">
			<li><p style="background:url(/images/slide1_lg.jpg) top center no-repeat; height:610px;"></p></li>
			<li><p style="background:url(/images/slide2_lg.jpg) top center no-repeat; height:610px;"></p></li>
		</ul>

		<script type="text/javascript">
			$(document).ready(function(){
				$('#visual').bxSlider({
					mode:'fade',
					autoControls: true
				});
			});
		</script>
	</div><!--// visual_wrap -->

	<div class="member_info">
		<div class="inner clfix">
			<div class="member_title"><p>CUSTOM LAB.</p></div>
			<div class="memberBox member_1">
				<a href="/sub.ejs?tbl=info">
					<h2>회사소개</h2>
					<p>Information</p>
					<span class="btn_more"><img src="/images/btn_more_arrow.png" alt="" /></span>
				</a>
			</div><!--// member_1 -->
			<div class="memberBox member_2">
				<a href="/sub.ejs?tbl=service">
					<h2>서비스소개</h2>
					<p>Service</p>
					<span class="btn_more"><img src="/images/btn_more_arrow.png" alt="" /></span>
				</a>
			</div><!--// member_2 -->
			<div class="memberBox member_3">
				<a href="/sub.ejs?tbl=reference">
					<h2>레퍼런스</h2>
					<p>Reference
					<!-- <form action="/kakaoPay" method="post">
						<input type="submit" name="e" value="e">
					</form> -->
					</p>
					<span class="btn_more"><img src="/images/btn_more_arrow.png" alt="" /></span>
				</a>
			</div><!--// member_3 -->
		</div><!--// inner -->
	</div><!--// member_info -->

	<%- include ("notice.ejs") %>
	<%- include ("product.ejs") %>
		</div><!--// left_contents -->

		<div class="right_contents">
			<%- include ("gallery.ejs") %>
		</div><!--// right_contents -->
	</div><!--// main_container -->

	<div id="footer">
	  <%- include ("footer.ejs") %>
	</div><!--// footer -->
</div><!--// wrap -->


</DIV>
<DIV ID="objSelection">
</DIV>

</body>
<script>
  window.kakaoAsyncInit = function() {
    Kakao.Channel.createAddChannelButton({
      container: '#kakao-talk-channel-add-button',
    });
  };
  window.kakaoAsyncInit = function() {
    Kakao.Channel.createChatButton({
      container: '#kakao-talk-channel-chat-button',
    });
  };

  (function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = 'https://developers.kakao.com/sdk/js/kakao.channel.min.js';
    fjs.parentNode.insertBefore(js, fjs);
  })(document, 'script', 'kakao-js-sdk');
</script>
</html>
